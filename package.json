{
    "name": "react-template",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "yarn": "1.22.4",
        "node": "12.16.1"
    },
    "scripts": {
        "start": "cross-env NODE_ENV=development nodemon --exec babel-node server --watch server",
        "start:prod": "cross-env NODE_ENV=production babel-node server",
        "prebuild:server": "rimraf server-dist",
        "build:server": "babel server --out-dir server-dist --copy-files",
        "prebuild": "rimraf dist",
        "build": "webpack --config webpack.config.prod.js",
        "clean": "rimraf dist server-dist",
        "lint": "eslint src server",
        "lint:sass": "stylelint src/**/*.sass"
    },
    "dependencies": {
        "chalk": "4.0.0",
        "cross-env": "7.0.2",
        "dotenv": "8.2.0",
        "express": "4.17.1"
    },
    "devDependencies": {
        "@babel/cli": "7.8.4",
        "@babel/core": "7.9.0",
        "@babel/node": "7.8.7",
        "@babel/plugin-proposal-optional-chaining": "7.9.0",
        "@babel/preset-env": "7.9.0",
        "@babel/preset-react": "7.9.4",
        "@hot-loader/react-dom": "16.13.0",
        "@reduxjs/toolkit": "1.3.3",
        "autoprefixer": "9.7.5",
        "babel-eslint": "10.1.0",
        "babel-loader": "8.1.0",
        "compression-webpack-plugin": "3.1.0",
        "core-js": "3.6.4",
        "css-loader": "3.4.2",
        "cssnano": "4.1.10",
        "eslint": "6.8.0",
        "eslint-plugin-react": "7.19.0",
        "file-loader": "6.0.0",
        "html-webpack-change-assets-extension-plugin": "1.2.0",
        "html-webpack-plugin": "4.0.4",
        "husky": "4.2.3",
        "mini-css-extract-plugin": "0.9.0",
        "node-sass": "4.13.1",
        "nodemon": "2.0.2",
        "postcss-loader": "3.0.0",
        "postcss-sass": "0.4.4",
        "prop-types": "15.7.2",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-hot-loader": "4.12.20",
        "redux-observable": "1.2.0",
        "rimraf": "3.0.2",
        "rxjs": "6.5.5",
        "sass-loader": "8.0.2",
        "style-loader": "1.1.3",
        "stylelint": "13.3.0",
        "trianglify": "3.0.0",
        "url-loader": "4.0.0",
        "webpack": "4.42.1",
        "webpack-cli": "3.3.11",
        "webpack-dev-middleware": "3.7.2",
        "webpack-hot-middleware": "2.25.0",
        "webpack-pwa-manifest": "4.2.0",
        "workbox-webpack-plugin": "5.1.2"
    },
    "browserslist": {
        "production": [
            "> 0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
