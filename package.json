{
    "name": "react-template",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "yarn": "1.22.0",
        "node": "12.16.0"
    },
    "scripts": {
        "start": "cross-env NODE_ENV=development babel-watch server",
        "start:prod": "cross-env NODE_ENV=production babel-node server",
        "prebuild:server": "yarn clean",
        "build:server": "babel server --out-dir server-dist --copy-files",
        "prebuild": "yarn clean",
        "build": "webpack --config webpack.config.prod.js",
        "clean": "rimraf dist server-dist",
        "lint": "eslint src server"
    },
    "dependencies": {
        "chalk": "3.0.0",
        "cross-env": "7.0.0",
        "dotenv": "8.2.0",
        "express": "4.17.1"
    },
    "devDependencies": {
        "@babel/cli": "7.8.4",
        "@babel/core": "7.8.4",
        "@babel/node": "7.8.4",
        "@babel/preset-env": "7.8.4",
        "@babel/preset-react": "7.8.3",
        "@hot-loader/react-dom": "16.11.0",
        "babel-loader": "8.0.6",
        "babel-watch": "7.0.0",
        "compression-webpack-plugin": "3.1.0",
        "core-js": "3.6.4",
        "eslint": "6.8.0",
        "eslint-plugin-react": "7.18.3",
        "html-webpack-change-assets-extension-plugin": "1.2.0",
        "html-webpack-plugin": "3.2.0",
        "husky": "4.2.1",
        "react": "16.12.0",
        "react-dom": "16.12.0",
        "react-hot-loader": "4.12.19",
        "rimraf": "3.0.2",
        "webpack": "4.41.6",
        "webpack-cli": "3.3.10",
        "webpack-dev-middleware": "3.7.2",
        "webpack-hot-middleware": "2.25.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint"
        }
    },
    "browsersList": [
        "not ie",
        "> 1%"
    ]
}
